{
  "name": "react-native-firework-sdk-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios_pod_install": "pod-install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --scheme FireworkSdkExample --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "react-native-firework-sdk": "2.17.0-beta.9",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@reduxjs/toolkit": "^2.8.2",
    "react": "19.0.0",
    "react-hook-form": "^7.56.4",
    "react-native": "0.78.2",
    "react-native-base64": "^0.2.1",
    "react-native-elements": "^3.4.3",
    "react-native-floating-action": "^1.22.0",
    "react-native-restart": "^0.0.27",
    "react-native-root-toast": "^4.0.1",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.10.0",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-icons": "^10.2.0",
    "react-native-visibility-aware-view": "^0.4.1",
    "react-native-webview": "^13.13.5",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "shopify-buy": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@types/react": "^19.0.0",
    "@types/react-native-base64": "^0.2.2",
    "@types/redux-logger": "^3.0.13",
    "@types/shopify-buy": "^3.0.0",
    "pod-install": "^0.3.9"
  },
  "engines": {
    "node": ">=18"
  }
}
